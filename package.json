    "@types/chai": "^5.0.1",
    "@types/json-bigint": "^1.0.4",
    "@types/mocha": "^10.0.10",
    "@types/request": "^2.48.12",
    "@types/string-hash": "^1.1.3",
    "@types/ws": "^8.18.0",
