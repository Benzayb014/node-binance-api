    "mocha": "^11.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^14.0.1",
    "nyc": "^17.1.0",
    "ts-jest": "^29.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
